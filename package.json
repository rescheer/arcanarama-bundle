{
  "name": "arcanarama-bundle",
  "version": "0.0.0",
  "description": "A NodeCG Bundle built for the Arcanarama Twitch livestream.",
  "homepage": "https://github.com/rescheer/arcanarama-bundle",
  "author": {
    "name": "Robert Scheer",
    "email": "robertedwinscheer@gmail.com",
    "url": "https://github.com/rescheer"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "prettier": {
    "singleQuote": true
  },
  "browserlist": "last 2 chrome versions",
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "assetCategories": [
      {
        "name": "assetUtilSponsors",
        "title": "Utility - Sponsors",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "assetUtilGiveaways",
        "title": "Utility - Giveaways",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "assetUtilCharacters",
        "title": "Utility - Character Cards",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "assetUtilMisc",
        "title": "Utility - Misc",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "utilityController",
        "title": "Utility Controller",
        "width": 6,
        "file": "utilityController.html",
        "headerColor": "#525F78",
        "workspace": "Main"
      }
    ],
    "graphics": [
      {
        "file": "utility.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "scripts": {
    "watch": "parcel watch --public-url ./",
    "build": "parcel build --public-url ./",
    "start": "npm --prefix ./../../ run start"
  },
  "targets": {
    "dashboard": {
      "source": "src/dashboard/utilityController.html",
      "distDir": "./dashboard"
    },
    "graphics": {
      "source": "src/graphics/utility.html",
      "distDir": "./graphics"
    }
  },
  "repository": "https://github.com/rescheer/arcanarama-bundle.git",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.0",
    "flux": "^4.0.3",
    "formik": "^2.2.9",
    "gsap": "^3.10.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
